{
  "env": {
    "dev": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ]
      ]
    },
    "build": {
      "presets": [
        "@babel/preset-env"
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "vue"
      ]
    }
  },
  "plugins": [
    "transform-vue-jsx"
  ]
}
